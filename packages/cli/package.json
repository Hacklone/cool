{
  "name": "@node-cool/cli",
  "description": "Cool server framework CLI",
  "version": "1.0.15",
  "author": "Barna Toth",
  "bin": {
    "node-cool": "./bin/run"
  },
  "bugs": "https://github.com/Hacklone/node-cool/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "gulp-replace": "^1.0.0",
    "map-stream": "0.0.7",
    "tslib": "^1.13.0",
    "vinyl-fs": "^3.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/gulp-replace": "0.0.31",
    "@types/node": "^10.17.29",
    "@types/vinyl-fs": "^2.4.11",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Hacklone/node-cool",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "node-cool",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Hacklone/node-cool",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme && rm -rf ./src/blueprint/node_modules && cp -R ./src/blueprint ./lib/blueprint",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
