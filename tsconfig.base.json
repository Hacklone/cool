{
  "compilerOptions": {
    "declaration": true,
    "stripInternal": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strict": true,
    "strictPropertyInitialization": true,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es5",
    "types": [
      "node",
    ],
    "lib": [
      "es2020",
    ],
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "inlineSources": true,
    "importHelpers": true
  }
}